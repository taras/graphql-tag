{
  "name": "@taras/graphql-tag",
  "version": "0.1.2",
  "license": "MIT",
  "exports": "./mod.ts",
  "lint": {
    "include": ["./**/*.ts"],
    "exclude": ["./**/*.js"]
  },
  "fmt": {
    "singleQuote": true,
    "semiColons": false,
    "useTabs": false,
    "indentWidth": 2,
    "lineWidth": 120
  },
  "tasks": {
    "test": "deno test --no-check --allow-net --allow-read --allow-write --coverage=coverage",
    "cov": "deno coverage coverage --lcov > coverage.lcov"
  },
  "imports": {
    "graphql": "npm:graphql@16.8.1",
    "expect": "jsr:@std/expect@^1",
    "testing": "jsr:@std/testing@^1/bdd"
  }
}
